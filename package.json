{
  "name": "covid-19_projet",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss dist/css/style.css -w",
    "server": "live-server --port=8181 --browser=chrome",
    "start": "concurrently \"npm run server\" \"npm run watch:sass\"",

    "compiler:sass": "node-sass sass/main.scss dist/css/style.comp.css",
    "concat:css": "concat -o dist/css/style.concat.css dist/css/all.css dist/css/style.comp.css",
    "prefix:css": "postcss dist/css/style.concat.css --use autoprefixer -o dist/css/style.prefix.css",
    "compress:css": "node-sass dist/css/style.prefix.css dist/css/style.css --output-style compressed",
    "building": "npm-run-all compiler:sass concat:css prefix:css compress:css",

    "deploying": "==============deploy==============",
    "deploy": "npm-run-all building-dev building-prod",
    "fin-deploying": "==============================",

    "Hebergement": "=====================================",
    "deploy": "gh-pages -d dist",
    "fin-Hebergement": "/=====================================/"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amine-dope/covid-19_projet.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amine-dope/covid-19_projet/issues"
  },
  "homepage": "https://github.com/amine-dope/covid-19_projet#readme",
  "devDependencies": {
    "autoprefixer": "^9.7.5",
    "concat": "^1.0.3",
    "concurrently": "^5.1.0",
    "gh-pages": "^2.2.0",
    "live-server": "^1.2.1",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0"
  }
}
